// func main (){
//   Looping()
// }
func Looping(){
  for a:=1; a<=5; a++{
    for b:=1; b<=5; b++{
      if b == 1 || b ==5 {
            fmt.Print("0")
      }else {
      if  a == 1 || a ==5 {
              fmt.Print("0")
      }else{
        fmt.Print(" ")
      }


      }

    }
    fmt.Println()

  }
}




