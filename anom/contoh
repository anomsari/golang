	fmt.Print(jumlah )
	fmt.Println(" Bilangan genap ")

default:
		fmt.Printf("hap")
	}
}


type Joy struct{
    Nama    string
    Sekolah string
}

func (masuk Joy)String() string{
  return fmt.Sprintf("Nama Saya  %v, Sekolah saya %v")
}

// var a,b,c = 5,6,4
func main() {
  masuk := map[string]Joy{
    "Nama Saya"     : (Nama :"Anomsari", Sekolah :"rasta"),
  }
  for name, data := range masuk{
    fmt.Printf("namaku %v :%v \n", name, data)
  }
}


